	require.async(["common:widget/lib/gmu/zepto/zepto.js"],function($){
		var statParam = "s=e4f4ba8d3290184788bc773d1643b8f0";
		if(statParam){
			$(".w-goods a").live("click",function(e){
				var ajaxStatParam = $(this).parent().data("s");
				/*判断是否为ajax分页的数据*/
				if(ajaxStatParam){
					statParam = ajaxStatParam;
				}
				var self = $(this);
				e.preventDefault(), window.location = $(this).attr("href")+"?"+statParam;
			});
		}
	});
