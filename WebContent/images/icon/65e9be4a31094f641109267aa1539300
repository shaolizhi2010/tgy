!function(){require.async(["common:widget/ui/suggestion/suggestion.js", "common:widget/lib/gmu/zepto/zepto.js", "common:widget/ui/usertrack/usertrack.js"], function (Sug, $, usertrack){
    // For Smarty
	
    var hotbrand = ["\u706b\u9505","\u81ea\u52a9\u9910","\u4fbf\u5b9c\u574a","\u8001\u8bda\u4e00\u9505","\u4e01\u4e01\u6d0b\u56de\u8f6c\u81ea\u52a9\u706b\u9505"];
	if (hotbrand == null) {
        hotbrand = new Array();
    }

    // 最多显示12个
    if(hotbrand.length > 12){
        hotbrand.length = 12;
    }
	
	//hotword统计的rid
	var hotword_rid = "130e7b7d81f8bd6a5495207ed9d85206";
	
    // For Html
    var $searchInput    = $(".j-search-input"),
        $suggestWrapper = $(".j-search-suggestion"),
        $searchClearBtn = $("#j-clear-input"),
        $searchForm     = $(".j-search-form");
        
    // For Css      
    var clearBtnClassName = "clean";
    
    // For nsClick 与统计解耦
    var sendStr = 'action=search&element_type=filter';
    var nsSearchClearBtn  = sendStr+"&area=searchFuc&element=clearInput",    
        nsSearchSubmitBtn = sendStr+"&area=search&element=###";
        
    // For Url  与后端Url解耦
    
    // 绑定 sug 功能
    Sug.suggest({
        $input      : $searchInput,
        $wrapper    : $suggestWrapper,  
        hotbrand    : hotbrand, 
		hotword_rid	: hotword_rid
    });
    
    // 是否展示 清除输入按钮
    $searchInput.on("input", function (){
        var toggleOn;
        
        if (this.value !== "") {
            toggleOn = true;
        } else {
            toggleOn = false;
        }
        
        $searchClearBtn.toggleClass(clearBtnClassName, toggleOn);

    });
    

    // 清除输入 功能
    $searchClearBtn.on("tap", function (){
        usertrack.send(nsSearchClearBtn);
        $searchInput.val("").trigger("input");
    });
    
    // 代理用户提交行为
    $searchForm.on("submit", function(evt){
        evt.preventDefault();
        
        var value = $searchInput.val();
    
        var nsContent = nsSearchSubmitBtn.replace("###", value || "noKeyWord");
        usertrack.send(nsContent);

        Sug.search(value);
    });
});
}();
