!function(){    require.async(["common:widget/lib/gmu/zepto/zepto.js", "common:widget/ui/cookie/cookie.js", "common:widget/ui/dialog/dialog.js"], function ($, Cookie, Dialog) {
        var hasSuccess = Cookie.get("bind_success");
        if (!hasSuccess) {
            return;
        }

        //清除一下
        Cookie.remove("bind_success");

        var DIALOG_WIDTH = 280;
        //获取模板
        var $upDialog = $("#update_passport_dialog_callback_wrap");
        var TMPL = $upDialog.html();
            TMPL = TMPL.replace('data-src','src');
        $upDialog.remove();
        var dialogObject = Dialog({
                hideHeader : true,
                content : TMPL,
                width : DIALOG_WIDTH,
                showCloseBtn : false,
                onClose : function () {
                    $(".u-close").remove();
                    return false;
                }
            });

        var $dialog = $(dialogObject.target);
        $dialog.attr("id", "update_passport_dialog_callback");
        var $closeBtn = $('<div class="u-close"></div>').appendTo($("body"));
        
        function _fix_dialog(){
            var mt =  0 - $dialog.height()/2;
            
            $dialog.css({"margin-top" : mt});
            var cm_top = parseInt(mt) - 10 + "px";
            var cm_left = parseInt($dialog.css("margin-left")) + DIALOG_WIDTH - 15 + "px";
            
            $closeBtn
            .css({
                "margin-top" : cm_top,
                "margin-left" : cm_left,
            });
        };
        //无法准确预计图片何时加载完成，因此监听image load的同时，200毫秒后再次计算一次
        $dialog.find(".u-title-img").on("load", function(){
            _fix_dialog();
        });
        setTimeout(function () {
            _fix_dialog();
        }, 200);
        
        //set close button
        var cm_top = parseInt($dialog.css("margin-top")) - 10 + "px";
        var cm_left = parseInt($dialog.css("margin-left")) + DIALOG_WIDTH - 15 + "px";
        $closeBtn
        .on("click", function () {
            dialogObject.destory();
            $closeBtn.remove();
        });

        $dialog.on("touchmove", function (event) {
            event.preventDefault();
            event.stopPropagation();
        });

        var userPhone = "";
        var userEmail = "";
        var userName = "";

        var $container = $dialog.find(".u-container");
        if (userPhone && userPhone != 0) {
            userPhone = userPhone.substring(0, 3) + "****" + userPhone.slice(-4);
            $container.append('<p class="u-title small low">' + userPhone + '</p>');
        }
        if (userEmail) {
            var sp_email = userEmail.split("@");
            if (sp_email.length === 2) {
                if (sp_email[0].length > 4) {
                    var enStr = "";
                    for (var i = sp_email[0].length - 4; i--; i > 0) {
                        enStr += "*";
                    }
                    userEmail = sp_email[0].slice(0, 2) + enStr + sp_email[0].slice(-2) + "@" + sp_email[1];
                } else {
                    var enStr = "";
                    for (var i = sp_email[0].length - 2; i--; i > 0) {
                        enStr += "*";
                    }
                    userEmail = sp_email[0].slice(0, 2) + enStr + "@" + sp_email[1];
                }
                $container.append('<p class="u-title small low">' + userEmail + '</p>');
            }
        }
        if (userName) {
            $container.append('<p class="u-title small low">' + userName + '</p>');
        }
    });
}();
