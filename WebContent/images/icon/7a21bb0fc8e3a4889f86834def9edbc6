<script type="text/javascript">
var akamaiLenovoFeo = false;
var lmd={
  actualBoxNum:"",
  bu:"",
  bundleID:"",
  businessType:"Public",
  chanprefix:"Public",
  country:"US",
  cc:"",
  customerID:"",
  discountCode:"",
  eCoupon:"",
  eDealCode:"",
  EPPserial:"",
  errorReferrer:"",
  events:"",
  loginDomain:"",
  pageCategory:"",
  pageNameSearchTerms:"",
  pageType:"", //set by error pages
  paymentMethod:"",
  products:"",
  purchaseID:"",
  registrationSrc:"",
  repID:"",
  reportSuites:"lenovoamericas",
  sapRoutes: {"all":"public",
           "pd":"",
           "p":"",
           "t":"",
           "td":""},
  sbbID:"",
  server:"LENWEB16.LENI2.COM",
  shipNTax:"",
  shippingType:"",
  shop:"Shop",
  siteName:"United States",
  sspx:1,
  state:"",
  store:"",
  storeID:"US Public Shop",
  suites: {"all":"",
           "pd":"lenovoamericasdev",
           "p":"lenovoamericas",
           "t":"lenovoamericas",
           "td":"lenovoamericasdev"},
  teleAccount:"lenovoamericas",
  teleCookieName:"iss-telesales",
  teleRole:"false",
  wishlistID:"",
  wishlistPageName:"",
  zip:""
};


(function(){
  
  if(lmd.events)
  {
    lmd.events = lmd.events.toLowerCase();
  }

  lmd.cc = "US".toLowerCase();
  lmd.bu = lmd.sapRoutes.p.toLowerCase();

  if(typeof(getCookie) != "undefined" && lmd.teleCookieName != "")
  {
    /* determine if we need to use telesales or public account and route */
    var tSuite = getCookie(lmd.teleCookieName);
    if(tSuite != "" && tSuite != null && lmd.sapRoutes.t != "" && lmd.teleAccount != "")
    {
      lmd.cc = "US".toLowerCase();
      lmd.bu = "tele";
    }    
  }
  
  if(lmd.repID != "")
  {
    lmd.cc = "US".toLowerCase();
    lmd.bu = "tele";
  }
  
  if(lmd.bu == "" && lmd.sapRoutes.all != "")
  {
    lmd.cc = "US".toLowerCase();
    lmd.bu = lmd.sapRoutes.all.toLowerCase();
  } 
  if(true == false)
  {
    lmd.bu += "dev";
  }  
})();



if(false && lmd.events==""){ lmd.events="event57"; }

if(!window.applyCustomLmd)
{
  function applyCustomLmd(triggerLmd)
  {
    if(window.customLmd)
    {
      if(!window.lmd)
      {
        lmd={}; //create the lmd object if it does not exist
      }
      for(var key in customLmd)
      {
        checkCustomLmd(key, customLmd[key], lmd[key])
      }    
    
      if(triggerLmd && window.s_ta)
      {
        if(window.console && window.console.log && window.JSON && window.JSON.stringify)
        {
          console.log("custom lmd > " + JSON.stringify(customLmd, undefined, 2));
        }
        s_ta();      
      }
      window.customLmd = null;
    }
  }

  function checkCustomLmd(lmdKey, lmdPropVar, lmdValue)
  {
    //lmdKey = property name
    //lmdProp = lmd property, e.g. "products" or "events".
    //lmdValue = default value from JDA trending code for this page    
    var customLdmValue = "";
    try
    {
      //check if this page contains a custom data array. It needs to be defined before this code.
      //sample array: var customLmd = {"events":"scProdView","products":"2436CTO"}
      //this array defines custom data for lmd property "events" and "products".
      if(window.customLmd)
      {
        //array exists. Get possible value by property name, e.g. "events", "products" from custom array
        if(lmdPropVar != null)
        {
          //there are a non-null value. Check which property it was (they have different output in lmd object
          switch(lmdKey)
          {
            case "events": 
            {
              //"events" property. Check if custom data length is > 0 characters (not blank), and if jda value is non blank.
              //in this case custom value must be prefixed with a comma, as events is a comma separated list of values: event1,event2,....            
              if(lmdPropVar.length > 0 && lmdPropVar.charAt(0) != ',' && lmdValue.length > 0)
              {
                //non blank custom value, and jda default value non-blank as well. Prefix with a comma.
                lmdPropVar = ',' + lmdPropVar;
              }            
              customLdmValue = lmdPropVar.toLowerCase();
              break;            
            }
            case "OrderProducts":
            {
              
              lmdValue = null;
              lmdKey = "products";
              customLdmValue = lmdPropVar + lmd.shipNTax;
              break;
            }
            case "products":
            {
              //"products" property. 
              //loop products list, should be separated by comma
              var prodString = "";
              var prods = lmdPropVar.split(",");
              for(var p=0;p<prods.length;p++)
              {
                if(prodString.length > 0)
                {
                  prodString += ",";
                }
                prodString += ";" + prods[p] + ";;;";
              }
              customLdmValue = (lmdValue != null && lmdValue != "")?","+prodString:prodString;
              break;
            }
            default:
            {
              //neither case handled, so add custom value as is.
              customLdmValue = lmdPropVar;
            }
          }        
        }
      }
    }
    catch(err)
    {
      //in case of error print to console for debugging
      if(window.console)
      {
        console.log("Trending error: " + err);
      }
    }
  
    //if lmd value was null, set custom value as is
    if(lmdValue == null)
    {
      lmd[lmdKey] = customLdmValue;
    }
    else
    {
      //jda default value was not null. catenate jda value and custom value.
      lmd[lmdKey] = lmdValue + customLdmValue;
    }
  }
}
</script>


