<div id="rollovercart">
	<div id="rollovercartContents" class="cf">
		<ul id="rollovercartContentsItems"></ul>
	</div>
	<div id="rollovercartFooter" class="cf">
		<a id="rollovercartViewCart" href="JavaScript:startSolution('//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');" class="button-standard button-full button-called-out">
