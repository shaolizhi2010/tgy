<script type="text/javascript">
	/** Seting Cart count in the cookie for dotcom pages **/
	var cart_domain = location.hostname.split('.').slice(-2).join('.'); // to get leni2.com or lenovo.com
	
	document.cookie="_CartCount="+0+"; domain=." + cart_domain + "; path=/";
	/** END **/
	function appendRowToCartRollover(contentItemsID, item, link) {
		$('#' + contentItemsID).append(
			$('<li>').append(
				$('<a>').attr('href', link).append(
					$('<img>').attr('class', 'rollovercartItemImg').attr('src', item.img)
				).append(
					$('<span>').attr('class', 'rollovercartItemName').text(item.description.replace(/\+/g,' '))
				).append(
					$('<span>').attr('class', 'rollovercartItemQty').attr('value', item.qty).text('Quantity: ' + item.qty + '')
				)
			)
		);
	}

    function getCookie(b){var c,a,e,d=document.cookie.split(";");for(c=0;c<d.length;c++){a=d[c].substr(0,d[c].indexOf("="));e=d[c].substr(d[c].indexOf("=")+1);a=a.replace(/^\s+|\s+$/g,"");if(a==b){return decodeURIComponent(e)}}}

	function updateCartRollover(cartLinkId, rolloverId, contentItemsID ) {
		var site = 'web';
		var cookieCart = (typeof getCookie == "function") ? getCookie('ISSUser' + site).split('|')[4] : '';
		var rollovercartItemLinks = [], rollovercartItemImages = [];


      

		if (cookieCart && cookieCart.length !== 0) {
			var jCart = $.parseJSON(cookieCart);
			var itemCount = 0;
			if (jCart && typeof jCart.items === 'object') {
				if ($.isArray(jCart.items)) {
					//itemCount = jCart.items.length;

					for (var i=0;i<jCart.items.length;i++) {
						var item = jCart.items[i],
							link = "JavaScript:startSolution('//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');";

						item.img = (typeof rollovercartItemImages[i] != 'undefined' && rollovercartItemImages[i].length)
							? rollovercartItemImages[i] : item.img;

						if (typeof(item) != "function")
						{
						  appendRowToCartRollover(contentItemsID, item, link);
						  itemCount += item.qty;
						}
					}
				} else {
					var item = jCart.items,
						link = "JavaScript:startSolution('//shop.lenovo.com/SEUILibrary/controller/e/web/LenovoPortal/en_US/cart.workflow:ShowCart?shopping-menu-selected-line=cart');";
					    item.img = "".replace(/http[s]?:/g,'');

					itemCount += item.qty;
					appendRowToCartRollover(contentItemsID, item, link);
				}

				$(".bar_2-link-cart").find('.number-flag').empty().html(''+itemCount+'').show();

				var cartLink = $(".link-wrapper");
				  $.each(cartLink,function(){
					if($(this).find(".cartlink" ).length > 0){
					  $(".cartrollover").appendTo($(this).parent());
					  if($(window).width() > 816){
					  $(this).children().addClass("link-hasChildren");
					  $(this).children().click(function(){$("#rollovercart").css("display","block");});
					  }
				   }
				  });

				  var counter = $(".cartlink");
				  $.each(counter,function(){
							$(this).children().append("<span class='counter'></span>");
							$(this).children().find('.counter').empty().html('&nbsp;('+itemCount+')').show();
					});
			}
		}
	}

	try {
		updateCartRollover('util-currentcart', 'rollovercart', 'rollovercartContentsItems');
	} catch (error) {
		if (window.debugAllowed && !!window.console) {
			console.warn('Error in updateCartRollover');
			console.error(error.message);
			console.log(error);
		}
	}
</script>
<!-- end persistent cart -->

