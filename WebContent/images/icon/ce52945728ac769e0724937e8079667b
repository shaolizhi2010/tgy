    <script type="text/javascript">
        function AddBug() {
            var vl = $("#textarea").val();
            if ($.trim(vl).length > 0) {
                $("#subBtn").attr("disabled", "disabled");
                $.ajax({
                    type: "POST",
                    url: "/App_Services/BugService.asmx/AddBug",
                    dataType: "json",
                    contentType: "application/json;utf-8",
                    data: "{str: '" + vl + "'}",
                    timeout: 10000,
                    error: function() {
                        $("#subBtn").removeAttr("disabled");
                        window.location = "/";
                    },
                    success: function(re) {
                        $("#subBtn").removeAttr("disabled");
                        alert('谢谢，提交成功～');
                        window.location = "/";
                    }
                });
            } else {
                alert('请描述出错前的操作步骤');
            }
        }    
    </script>

